{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ES2022",
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["vite/client", "react", "react-dom"]
  },
  "include": ["src"]
}
